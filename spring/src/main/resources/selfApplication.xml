<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
            http://www.springframework.org/schema/context
            http://www.springframework.org/schema/context/spring-context-3.0.xsd">

    <bean id="selfService" class="cn.leon.selfContext.SelfService">
        <property name="name" value="jeysin"/>
        <property name="age" value="13"/>
        <property name="user" ref="user"/>
    </bean>

    <bean id="user" class="cn.leon.selfContext.User">
        <property name="selfService" ref="selfService"/>
        <property name="name" value="yize"/>
    </bean>

</beans>